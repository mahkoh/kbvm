xkb_keymap {
    xkb_keycodes {
        include "generated"
    };
    xkb_symbols {
        key <a> {
            [ a ],
        };
        key <b> {
            lock,
            [ b ],
            [ SetMods(mods = Shift) ],
        };



        key <media_repeat> {
            repeatLastKey,
        };
    };
};
