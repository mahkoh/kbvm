xkb_keymap {
    xkb_keycodes {
        include "generated"
    };
    xkb_symbols {
        key <i> {
            [ i, I ],
        };
        key <leftshift> {
            [ SetMods(mods = Shift, clearLocks) ]
        };
        key <capslock> {
            [ LockMods(mods = Shift) ]
        };
    };
};
