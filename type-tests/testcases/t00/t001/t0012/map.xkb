xkb_keymap {
    xkb_keycodes {
        include "generated"
    };
    xkb_symbols {
        key <a> {
            [ a, A ],
            [ b, B ],
        };
        key <leftshift> {
            [
                {
                    SetMods(mods = Shift),
                    SetGroup(group = 2),
                }
            ]
        };
    };
};
