xkb_keymap {
    xkb_keycodes {
        <a> = 1;
        <b> = 2;
    };
    xkb_symbols {
        key <a> {
            [ { a, b } ],
            [ { SetMods(mods = Mod2), SetMods(mods = Mod3) } ],
        };
        key <b> {
            [ a ],
            [ SetMods(mods = Mod2), SetMods(mods = Mod3) ],
        };
    };
};
