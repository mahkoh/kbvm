xkb_keymap {
    xkb_keycodes {
        minimum = 8;
        maximum = 8;

        virtual indicator 1 = "B";
        virtual indicator 2 = "A";
    };

    xkb_types {
        virtual_modifiers Dummy;
    };

    xkb_compat {
        interpret VoidSymbol {
            repeat = false;
        };
    };

    xkb_symbols {
    };
};
