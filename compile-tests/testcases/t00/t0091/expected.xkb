xkb_keymap {
    xkb_keycodes {
        minimum = 8;
        maximum = 8;

        virtual indicator 1 = "A";
    };

    xkb_types {
        virtual_modifiers Dummy;
    };

    xkb_compat {
        indicator "A" {
            modifiers = Mod1;
            groups = 0x00000001;
            whichGroupState = Locked;
        };

        interpret VoidSymbol {
            repeat = false;
        };
    };

    xkb_symbols {
    };
};
