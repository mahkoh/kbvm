xkb_keymap {
    xkb_keycodes {
        minimum = 8;
        maximum = 255;

        indicator 1 = "DUMMY";

        <a> = 1;
        <b> = 2;
    };

    xkb_types {
        virtual_modifiers Dummy;

        type "ONE_LEVEL" {
            modifiers = None;
            level_name[Level1] = "Any";
            map[None] = Level1;
        };
    };

    xkb_compat {
        interpret VoidSymbol {
            repeat = false;
        };
    };

    xkb_symbols {
        modmap Mod1 { <a> };
        modmap Mod2 { <b> };

        key.repeat = true;

        key <a> {
            type[Group1] = "ONE_LEVEL",
            symbols[Group1] = [ A, B, C, D, E ],
            actions[Group1] = [ NoAction(), SetMods(modifiers = Mod1), SetMods(modifiers = Mod1), SetMods(modifiers = Mod1), SetMods(modifiers = Mod1) ]
        };
        key <b> {
            repeat = false,
            type[Group1] = "ONE_LEVEL",
            symbols[Group1] = [ A, B, C, D, E ],
            actions[Group1] = [ SetMods(modifiers = Mod1), NoAction(), NoAction(), NoAction(), NoAction() ]
        };
    };
};
