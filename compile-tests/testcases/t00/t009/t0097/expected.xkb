xkb_keymap {
    xkb_keycodes {
        minimum = 8;
        maximum = 255;

        indicator 1 = "DUMMY";

        <a> = 1;
    };

    xkb_types {
        virtual_modifiers Dummy;

        type "A" {
            modifiers = None;
            level_name[Level1] = "A";
        };
    };

    xkb_compat {
        interpret VoidSymbol {
            repeat = false;
        };
    };

    xkb_symbols {
        key.repeat = true;

        key <a> {
            type[Group1] = "A",
            symbols[Group1] = [ A ]
        };
    };
};
