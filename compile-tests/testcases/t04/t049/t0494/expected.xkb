xkb_keymap {
    xkb_keycodes {
        minimum = 8;
        maximum = 255;

        indicator 1 = "DUMMY";

        <a> = 1;
        <b> = 2;
        <c> = 3;
        <d> = 4;
        <e> = 5;
        <f> = 6;
    };

    xkb_types {
        virtual_modifiers Dummy;

        type "ONE_LEVEL" {
            modifiers = None;
            level_name[Level1] = "Any";
            map[None] = Level1;
        };
    };

    xkb_compat {
        interpret VoidSymbol {
            repeat = false;
        };
    };

    xkb_symbols {
        key.repeat = true;

        key <a> {
            repeat = false,
            repeatLastKey
        };
        key <b> {
            repeat = false
        };
        key <c> {
            repeat = false,
            repeatLastKey
        };
        key <d> {
            repeat = false
        };
        key <e> {
            repeatLastKey
        };
        key <f> {
            repeatLastKey,
            type[Group1] = "ONE_LEVEL",
            symbols[Group1] = [ a ]
        };
    };
};
