xkb_keymap {
    xkb_keycodes {
        minimum = 8;
        maximum = 255;

        indicator 1 = "DUMMY";

        <1> = 1;
    };

    xkb_types {
        virtual_modifiers Dummy;

        type "ONE_LEVEL" {
            modifiers = None;
            level_name[Level1] = "Any";
            map[None] = Level1;
        };
    };

    xkb_compat {
        interpret VoidSymbol {
            repeat = false;
        };
    };

    xkb_symbols {
        modmap Mod1 { <1> };
        modmap Mod2 { x }; // <1>

        key.repeat = true;

        key <1> {
            type[Group1] = "ONE_LEVEL",
            symbols[Group1] = [ x ]
        };
    };
};
