xkb_keymap {
    xkb_keycodes {
        minimum = 8;
        maximum = 255;

        indicator 1 = "DUMMY";

        <1> = 1;
    };

    xkb_types {
        virtual_modifiers Dummy;

        type "X" {
            modifiers = None;
            map[Shift] = Level2;
        };
    };

    xkb_compat {
        interpret VoidSymbol {
            repeat = false;
        };
    };

    xkb_symbols {
        modmap Mod1 { <1> };

        key.repeat = true;

        key <1> {
            type[Group1] = "X",
            symbols[Group1] = [ a, b ]
        };
    };
};
