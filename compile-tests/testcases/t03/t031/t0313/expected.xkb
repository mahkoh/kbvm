xkb_keymap {
    xkb_keycodes {
        minimum = 8;
        maximum = 8;

        indicator 1 = "DUMMY";

        <a> = 1;
    };

    xkb_types {
        virtual_modifiers Dummy;

        type "ONE_LEVEL" {
            modifiers = None;
            level_name[Level1] = "Any";
            map[None] = Level1;
        };
    };

    xkb_compat {
        interpret VoidSymbol {
            repeat = false;
        };
    };

    xkb_symbols {
        key.repeat = true;

        key <a> {
            type[Group1] = "ONE_LEVEL",
            type[Group4] = "ONE_LEVEL",
            type[Group5] = "ONE_LEVEL",
            type[Group6] = "ONE_LEVEL",
            type[Group7] = "ONE_LEVEL",
            symbols[Group1] = [ A ],
            symbols[Group4] = [ 0 ],
            symbols[Group5] = [ 3 ],
            symbols[Group6] = [ 9 ],
            symbols[Group7] = [ 0x0000000a ]
        };
    };
};
