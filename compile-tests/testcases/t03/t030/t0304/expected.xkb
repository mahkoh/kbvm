xkb_keymap {
    xkb_keycodes {
        minimum = 8;
        maximum = 255;

        virtual indicator 1 = "A";
    };

    xkb_types {
        virtual_modifiers Dummy;
    };

    xkb_compat {
        indicator "A" {
            modifiers = Mod1;
            whichModState = Base+Locked+Effective;
        };

        interpret VoidSymbol {
            repeat = false;
        };
    };

    xkb_symbols {
    };
};
