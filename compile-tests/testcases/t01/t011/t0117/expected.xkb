xkb_keymap {
    xkb_keycodes {
        minimum = 8;
        maximum = 8;

        indicator 1 = "DUMMY";

        <a> = 1;
    };

    xkb_types {
        virtual_modifiers Dummy;

        type "TWO_LEVEL" {
            modifiers = Shift;
            level_name[Level1] = "Base";
            level_name[Level2] = "Shift";
            map[Shift] = Level2;
        };
    };

    xkb_compat {
        interpret VoidSymbol {
            repeat = false;
        };
    };

    xkb_symbols {
        key.repeat = true;

        key <a> {
            repeat = false,
            type[Group2] = "TWO_LEVEL",
            symbols[Group2] = [ A, B ]
        };
    };
};
