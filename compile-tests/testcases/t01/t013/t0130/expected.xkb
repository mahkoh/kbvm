xkb_keymap {
    xkb_keycodes {
        minimum = 8;
        maximum = 255;

        indicator 1 = "DUMMY";
    };

    xkb_types {
        virtual_modifiers Dummy;
    };

    xkb_compat {
        interpret VoidSymbol {
            repeat = false;
        };
    };

    xkb_symbols {
        name[Group1] = "X";
        name[Group2] = "Y";
    };
};
